{
  "name": "landing-pad-dxp-backend",
  "version": "1.0.0",
  "description": "Backend API for Landing Pad DXP platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "db:init": "node src/scripts/initDb.js",
    "db:reset": "node src/scripts/initDb.js --force",
    "db:seed": "node src/scripts/initDb.js --no-migrate",
    "db:migrate": "node src/scripts/initDb.js --migrate --no-seed",
    "db:prod:init": "node src/scripts/initDb.js --production --migrate",
    "worker": "node bin/run-worker.js",
    "worker:dev": "nodemon bin/run-worker.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run worker:dev\""
  },
  "keywords": [
    "landing page",
    "website builder",
    "api",
    "express"
  ],
  "author": "Landing Pad DXP Team",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-slow-down": "^2.0.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cache": "^5.1.2",
    "openai": "^4.87.3",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "pg": "^8.14.0",
    "sequelize": "^6.35.1",
    "sqlite3": "^5.1.6",
    "stripe": "^14.5.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
